# target arch
UCLIBC_FORMAT_SHARED_FLAT=y
UCLIBC_HAS_FLOATS=y

# general
HAVE_SHARED=y
LDSO_LDD_SUPPORT=y
LDSO_CACHE_SUPPORT=n
HAS_NO_THREADS=y
MALLOC_STANDARD=y
UCLIBC_DYNAMIC_ATEXIT=y
UCLIBC_STRICT_HEADERS=n
UCLIBC_HAS_STUBS=n

# advanced
UCLIBC_LINUX_SPECIFIC=y

# networking
UCLIBC_HAS_NETWORK_SUPPORT=n
UCLIBC_HAS_IPV4=y
UCLIBC_HAS_IPV6=n
UCLIBC_USE_NETLINK=y
UCLIBC_HAS_RESOLVER_SUPPORT=y

# string
